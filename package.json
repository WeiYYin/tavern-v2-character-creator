{
  "name": "tavern-v2-character-creator",
  "version": "1.0.0",
  "description": "A simple character creator for Character Card Spec v2",
  "scripts": {
    "build": "npm run clean && parcel build src/index.html --dist-dir docs --public-url ~/",
    "launch": "npm run build && serve -s docs",
    "clean": "rimraf docs",
    "lint": "tslint -c tslint.json -p tsconfig.json -t stylish",
    "start": "parcel -p 3000 --no-cache src/index.html --dist-dir dist"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.8.10",
    "@types/react": "^18.2.34",
    "@types/react-dom": "^18.2.14",
    "parcel": "^2.10.2",
    "postcss": "^8.4.31",
    "process": "^0.11.10",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.3.5",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@hookstate/core": "^4.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead"
}
